{
    "name": "tss-react",
    "version": "4.4.1",
    "description": "Type safe CSS-in-JS API heavily inspired by react-jss",
    "repository": {
        "type": "git",
        "url": "git://github.com/garronej/tss-react.git"
    },
    "main": "index.js",
    "types": "index.d.ts",
    "module": "esm/index.js",
    "exports": {
        ".": {
            "import": "./esm/index.js",
            "require": "./index.js"
        },
        "./mui": {
            "import": "./esm/mui.js",
            "require": "./mui.js"
        },
        "./nextJs": {
            "import": "./esm/nextJs.js",
            "require": "./nextJs.js"
        },
        "./next": {
            "import": "./esm/next.js",
            "require": "./next.js"
        },
        "./compat": {
            "import": "./esm/compat.js",
            "require": "./compat.js"
        },
        "./mui-compat": {
            "import": "./esm/mui-compat.js",
            "require": "./mui-compat.js"
        },
        "./cssAndCx": {
            "import": "./esm/cssAndCx.js",
            "require": "./cssAndCx.js"
        }
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix"
        ],
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "author": "u/garronej",
    "license": "MIT",
